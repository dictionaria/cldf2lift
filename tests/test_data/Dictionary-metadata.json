{
    "@context": [
        "http://www.w3.org/ns/csvw",
        {
            "@language": "en"
        }
    ],
    "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#Dictionary",
    "dialect": {
        "commentPrefix": null
    },
    "tables": [
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#EntryTable",
            "dc:extent": 3,
            "tableSchema": {
                "columns": [
                    {
                        "datatype": {
                            "base": "string",
                            "format": "[a-zA-Z0-9_\\-]+"
                        },
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id",
                        "required": true,
                        "name": "lx_id"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#languageReference",
                        "required": true,
                        "name": "lang"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#headword",
                        "required": true,
                        "name": "lx"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#partOfSpeech",
                        "required": false,
                        "name": "ps"
                    },
                    {
                        "datatype": "string",
                        "name": "Variant_Form"
                    }
                ],
                "primaryKey": [
                    "lx_id"
                ]
            },
            "url": "entries.csv"
        },
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#SenseTable",
            "dc:extent": 6,
            "tableSchema": {
                "columns": [
                    {
                        "datatype": {
                            "base": "string",
                            "format": "[a-zA-Z0-9_\\-]+"
                        },
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id",
                        "required": true,
                        "name": "sn"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#description",
                        "required": true,
                        "name": "de"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#entryReference",
                        "required": true,
                        "name": "lx_ref"
                    },
                    {
                        "datatype": "string",
                        "name": "alt_translation1"
                    },
                    {
                        "datatype": "string",
                        "name": "alt_translation2"
                    }
                ],
                "foreignKeys": [
                    {
                        "columnReference": [
                            "lx_ref"
                        ],
                        "reference": {
                            "resource": "entries.csv",
                            "columnReference": [
                                "lx_id"
                            ]
                        }
                    }
                ],
                "primaryKey": [
                    "sn"
                ]
            },
            "url": "senses.csv"
        },
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ExampleTable",
            "dc:extent": 5,
            "tableSchema": {
                "columns": [
                    {
                        "datatype": {
                            "base": "string",
                            "format": "[a-zA-Z0-9_\\-]+"
                        },
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id",
                        "required": true,
                        "name": "xid"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#languageReference",
                        "required": true,
                        "name": "xlang"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#primaryText",
                        "required": true,
                        "name": "xv"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#analyzedWord",
                        "required": false,
                        "separator": "\\t",
                        "name": "xm"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#gloss",
                        "required": false,
                        "separator": "\\t",
                        "name": "xg"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#translatedText",
                        "required": false,
                        "name": "xe"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#metaLanguageReference",
                        "required": false,
                        "name": "xmlang"
                    },
                    {
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#comment",
                        "required": false,
                        "name": "xn"
                    },
                    {
                        "datatype": "string",
                        "separator": " ; ",
                        "name": "Sense_IDs"
                    },
                    {
                        "datatype": "string",
                        "name": "alt_translation1"
                    },
                    {
                        "datatype": "string",
                        "name": "alt_translation2"
                    }
                ],
                "foreignKeys": [
                    {
                        "columnReference": [
                            "Sense_IDs"
                        ],
                        "reference": {
                            "resource": "senses.csv",
                            "columnReference": [
                                "sn"
                            ]
                        }
                    }
                ],
                "primaryKey": [
                    "xid"
                ]
            },
            "url": "examples.csv"
        }
    ]
}
